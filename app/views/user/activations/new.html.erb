<h1>Resend activation</h1>
<% form_tag user_activations_path do %>

<fieldset>
  <p>

    What is the email address used to create your account?
  </p>
    <%= text_field_tag :email, "", :size => 50 %><br />
</fieldset>
<br/>
    <%= submit_tag 'Resend Activation Code', :disable_with => "Sending&hellip;" %>
<% end %>
<div id="toolbar"></div>